{
  "name": "@finanzas/web",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=25.0.0",
    "npm": ">=10.0.0"
  },
  "overrides": {
    "hono": "^4.10.3",
    "jspdf": "$jspdf"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:fast": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "clean": "rm -rf dist",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@casl/ability": "^6.7.5",
    "@casl/react": "^5.0.0",
    "@finanzas/db": "workspace:*",
    "@fullcalendar/core": "^6.1.20",
    "@fullcalendar/daygrid": "^6.1.20",
    "@fullcalendar/react": "^6.1.20",
    "@fullcalendar/timegrid": "^6.1.20",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@simplewebauthn/browser": "^13.2.2",
    "@tanstack/react-form": "^1.27.7",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-router": "^1.149.3",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/router-devtools": "^1.149.3",
    "@tanstack/zod-form-adapter": "^0.42.1",
    "@zenstackhq/server": "3.2.1",
    "@zenstackhq/tanstack-query": "3.2.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.19",
    "i18next": "^25.7.4",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "ky": "^1.14.2",
    "lucide-react": "^0.562.0",
    "papaparse": "^5.5.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-i18next": "^16.5.2",
    "recharts": "^3.6.0",
    "tailwind-merge": "^3.4.0",
    "use-debounce": "^10.1.0",
    "zod": "^4.3.5",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.51.4",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/router-plugin": "^1.149.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.8",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "^1.0.0",
    "concurrently": "^9.2.1",
    "daisyui": "^5.5.14",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "pino-pretty": "^13.1.3",
    "postcss-preset-env": "^10.6.1",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-checker": "^0.12.0",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.17"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --max-warnings 0",
      "prettier --write",
      "bash -c 'tsc --noEmit'"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
